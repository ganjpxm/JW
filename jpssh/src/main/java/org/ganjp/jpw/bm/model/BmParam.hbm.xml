<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.ganjp.jpw.bm.model.BmParam" table="bm_param">
<id name="paramId" column="param_id" type="java.lang.String" unsaved-value="">
<generator class="uuid"/>
</id>
<property name="paramCd" type="java.lang.String" column="param_cd"/>
<property name="paramName" type="java.lang.String" column="param_name"/>
<property name="paramTypeCd" type="java.lang.String" column="param_type_cd"/>
<property name="paramTypeName" type="java.lang.String" column="param_type_name"/>
<property name="displayNo" type="java.lang.Integer" column="display_no"/>
<property name="lang" type="java.lang.String" column="lang"/>
<property name="createDateTime" type="java.sql.Timestamp" column="create_date_time"/>
<property name="modifyTimestamp" type="java.sql.Timestamp" column="modify_timestamp"/>
<property name="dataState" type="java.lang.String" column="data_state"/>
</class>
</hibernate-mapping>
