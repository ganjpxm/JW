<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.ganjp.jpw.cm.model.CmVideo" table="cm_video">
<id name="videoId" column="video_id" type="java.lang.String" unsaved-value="">
<generator class="uuid"/>
</id>
<property name="videoName" type="java.lang.String" column="video_name"/>
<property name="videoFormat" type="java.lang.String" column="video_format"/>
<property name="title" type="java.lang.String" column="title"/>
<property name="thumbUrl" type="java.lang.String" column="thumb_url"/>
<property name="url" type="java.lang.String" column="url"/>
<property name="originUrl" type="java.lang.String" column="origin_url"/>
<property name="originWebsite" type="java.lang.String" column="origin_website"/>
<property name="tag" type="java.lang.String" column="tag"/>
<property name="displayNo" type="java.lang.Integer" column="display_no"/>
<property name="remark" type="java.lang.String" column="remark"/>
<property name="lang" type="java.lang.String" column="lang"/>
<property name="roleIds" type="java.lang.String" column="role_ids"/>
<property name="operatorId" type="java.lang.String" column="operator_id"/>
<property name="operatorName" type="java.lang.String" column="operator_name"/>
<property name="createDateTime" type="java.sql.Timestamp" column="create_date_time"/>
<property name="modifyTimestamp" type="java.sql.Timestamp" column="modify_timestamp"/>
<property name="dataState" type="java.lang.String" column="data_state"/>
</class>
</hibernate-mapping>
