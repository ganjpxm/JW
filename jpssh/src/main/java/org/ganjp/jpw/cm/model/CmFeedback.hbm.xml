<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.ganjp.jpw.cm.model.CmFeedback" table="cm_feedback">
<id name="feedbackId" column="feedback_id" type="java.lang.String" unsaved-value="">
<generator class="uuid"/>
</id>
<property name="feedbackType" type="java.lang.String" column="feedback_type"/>
<property name="section" type="java.lang.String" column="section"/>
<property name="subject" type="java.lang.String" column="subject"/>
<property name="content" type="java.lang.String" column="content"/>
<property name="operatorId" type="java.lang.String" column="operator_id"/>
<property name="operatorName" type="java.lang.String" column="operator_name"/>
<property name="createDateTime" type="java.sql.Timestamp" column="create_date_time"/>
<property name="modifyTimestamp" type="java.sql.Timestamp" column="modify_timestamp"/>
<property name="dataState" type="java.lang.String" column="data_state"/>
</class>
</hibernate-mapping>
