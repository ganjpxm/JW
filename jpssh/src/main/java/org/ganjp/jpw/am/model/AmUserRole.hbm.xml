<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.ganjp.jpw.am.model.AmUserRole" table="am_user_role">
<id name="userRoleId" column="user_role_id" type="java.lang.String" unsaved-value="">
<generator class="uuid"/>
</id>
<property name="roleId" type="java.lang.String" column="role_id"/>
<property name="userId" type="java.lang.String" column="user_id"/>
<property name="createDateTime" type="java.sql.Timestamp" column="create_date_time"/>
<property name="modifyTimestamp" type="java.sql.Timestamp" column="modify_timestamp"/>
<property name="dataState" type="java.lang.String" column="data_state"/>
</class>
</hibernate-mapping>
